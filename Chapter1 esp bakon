script.Parent.MouseButton1Click:Connect(function()
	local ESP = loadstring(game:HttpGet("https://kiriot22.com/releases/ESP.lua"))()

	ESP.Players = false
	ESP.Boxes = false
	ESP.Names = true
	ESP:Toggle(true)

	ESP:AddObjectListener(Workspace.CurrentMap.Chapter1.Utilities,{
		Name = "Red Key",
		CustomName = "Red Key",
		Color = Color3.fromRGB(255, 0, 4),
		IsEnabled = "RedKey",
	})
	ESP.RedKey = true

	ESP:AddObjectListener(Workspace.CurrentMap.Chapter1.Utilities,{
		Name = "Blue Key",
		CustomName = "Blue Key",
		Color = Color3.fromRGB(0, 206, 209),
		IsEnabled = "BlueKey",
	})
	ESP.BlueKey = true

	ESP:AddObjectListener(Workspace.CurrentMap.Chapter1.Utilities,{
		Name = "Orange Key",
		CustomName = "Orange Key",
		Color = Color3.fromRGB(255, 165, 0),
		IsEnabled = "OrangeKey",
	})
